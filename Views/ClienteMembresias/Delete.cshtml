@model Gimnasio_Brothers.Models.ClienteMembresium

@{
    ViewData["Title"] = "Eliminar";
}

<h1>Eliminar membresía</h1>

<h3>Está seguro que desea eliminar la membresía?</h3>
<div>
    <h4>Membresía de cliente</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Fecha de renovación
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaProxRenovacion)
        </dd>
        <dt class = "col-sm-2">
            Fecha inicio
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaInicio)
        </dd>
        <dt class = "col-sm-2">
            Fecha fin
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaFin)
        </dd>
        <dt class = "col-sm-2">
            Tipo membresía
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdcategoraMembresiaNavigation.Nombre)
        </dd>
        <dt class = "col-sm-2">
            Cliente
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdclienteNavigation.Apellidos)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdclienteMembresia" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index" class="btn btn-dark"><i class="bi bi-arrow-return-left"> Regresar a la lista</i></a>
    </form>
</div>
