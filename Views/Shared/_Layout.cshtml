<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Gimnasio Brothers</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.2/font/bootstrapicons.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.2/font/bootstrap-icons.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Gimnasio_Brothers.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/sweetalert2.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/datatables.min.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Gimnasio Brothers</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Inicio</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>

                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" role="button" ariahaspopup="true" aria-expanded="false">Empleados</a>
                            <div class="dropdown-menu">
                                
                                <a class="nav-link text-dark" asp-area="" asp-controller="Empleados" asp-action="Index">Empleados</a>                                
                                <a class="nav-link text-dark" asp-area="" asp-controller="Clases" aspaction="Index">Clases</a>
                                <a class="nav-link text-dark" asp-area="" asp-controller="Jornadas" aspaction="Index">Jornadas</a>
                                
                             </div>
                        </li>

                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" role="button" ariahaspopup="true" aria-expanded="false">Pedidos</a>
                            <div class="dropdown-menu">
								
                                <a class="nav-link text-dark" asp-area="" asp-controller="Pedidos" aspaction="Index">Pedidos</a>
                                <a class="nav-link text-dark" asp-area="" asp-controller="CategoriaProductos" aspaction="Index">CategoriaProductos</a>                                                                  
                                <a class="nav-link text-dark" asp-area="" asp-controller="ProductosVentas" aspaction="Index">ProductosVentas</a>
                                <a class="nav-link text-dark" asp-area="" asp-controller="Proveedores" aspaction="Index">Proveedores</a>
                                
                            </div>
                        </li>

                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" role="button" ariahaspopup="true" aria-expanded="false">Clases</a>
                            <div class="dropdown-menu">
                                
                                <a class="nav-link text-dark" asp-area="" asp-controller="Actividades" aspaction="Index">Actividades</a>                                                                                                                                                                                                                               
                                <a class="nav-link text-dark" asp-area="" asp-controller="Martriculas" aspaction="Index">Martriculas</a>
                                <a class="nav-link text-dark" asp-area="" asp-controller="Salas" aspaction="Index">Salas</a>
                               
                            </div>
                        </li>

                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" role="button" ariahaspopup="true" aria-expanded="false">Membresias</a>
                            <div class="dropdown-menu">
                                <a class="nav-link text-dark" asp-area="" asp-controller="CategoriaMembresias" aspaction="Index">CategoriaMembresias</a>
                                <a class="nav-link text-dark" asp-area="" asp-controller="ClienteMembresias" aspaction="Index">ClienteMembresias</a>
                                <a class="nav-link text-dark" asp-area="" asp-controller="Clientes" aspaction="Index">Clientes</a>
                                <a class="nav-link text-dark" asp-area="" asp-controller="FacturaClientes" aspaction="Index">FacturaClientes</a>
                                <a class="nav-link text-dark" asp-area="" asp-controller="SesionesUv" aspaction="Index">SesionesUv</a>
                            </div>
                        </li>                        

                    </ul>
                    <partial name="_LoginPartial" />
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - Gimnasio_Brothers - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/datatables.min.js" asp-append-version="true"></script>
    <script src="~/js/sweetalert2.all.min.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
