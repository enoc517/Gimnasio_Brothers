@model Gimnasio_Brothers.Models.SesionesUv

@{
    ViewData["Title"] = "Caracteristicas";
}

<h1>Caracteristicas De Sesiones UV</h1>
<br>
<div>
    <h4>Sesiones Uv</h4>
    <hr />
    
    <dl class="row">
        <dt class = "col-sm-2">
            Cantida De Sesiones
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CantidadSesiones)
        </dd>
        <dt class = "col-sm-2">
            Fecha de Sesion
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaSesion)
        </dd>
        <dt class = "col-sm-2">
            Hora de Sesion
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.HoraSesion)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Disponibles)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Disponibles)
        </dd>
        <dt class = "col-sm-2">
            Cliente Membresia
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdclienteMembresiaNavigation.IdclienteMembresia)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Idsesiones" class="btn btn-danger"> <i class="bi bi-pencil-fill"> Editar</i> </a> |
    <a asp-action="Index" class="btn btn-warning"> <i class="bi bi-arrow-return-left">Regresar a Sesiones UV</i></a>
</div>
