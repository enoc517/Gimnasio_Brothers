@model Gimnasio_Brothers.Models.Jornada

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Jornada</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FechaInicio)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaInicio)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FechaFin)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaFin)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.HorasTrabajadas)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.HorasTrabajadas)
        </dd>
        <dt class = "col-sm-2">
            Empleado
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdempleadoNavigation.Apellidos)
        </dd>
    </dl>
</div>
<br>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Idjornadas" class="btn btn-danger"> <i class="bi bi-pencil-fill"> Editar</i> </a> |
    <a asp-action="Index" class="btn btn-warning"><i class="bi bi-arrow-return-left">Regresar a la Lista</i></a>
</div>
